{
  "name": "Window saver",
  "description": "Save your active session tabs and split workplaces",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16":"icons/tab.png",
    "32":"icons/tab-big.png",
    "48":"icons/tab-big.png",
    "128":"icons/tab-big.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {}
}